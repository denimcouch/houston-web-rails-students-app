<h1> <%= @instructor.name %> </h1>

<h2>Students:</h2>
<p>
    <%@instructor.students.each do |student| %>
        <li>
            <%= link_to student.name, 
            student_path(student) %>
        </li>
    <% end %>
</p>

<%= button_to('Delete Instructor', instructor_url(@instructor), method: "delete") %>