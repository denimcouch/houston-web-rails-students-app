<%= form_for @student do |f|%>
<%=f.label :name%>
<%=f.text_field :name%><br>
<%=f.label :major%>
<%=f.text_field :major%><br>
<%=f.label :age%>
<%=f.number_field :age%><br>
<%=f.collection_select :instructor_id, @instructors, :id, :name, {include_blank: 'Select an Instructor'}%><br>
<%=f.submit%>
<% end %>